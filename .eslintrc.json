{
	"parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "extends": "airbnb",
    "rules": {
     	"indent": [2, "tab", {"SwitchCase": 1}],
        "no-tabs": 0,
        "react/jsx-indent": [2, "tab"],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    	"jsx-quotes": ["error", "prefer-single"]
    }
}
